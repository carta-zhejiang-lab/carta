        "sass-loader": "^12.3.0",
        "semver": "^7.3.5",
        "source-map-loader": "^3.0.0",
        "style-loader": "^3.3.1",
        "tailwindcss": "^3.0.2",
        "terser-webpack-plugin": "^5.2.5",
        "usehooks-ts": "^3.1.0",
        "webpack": "^5.64.4",
        "webpack-dev-server": "^4.6.0",
        "webpack-manifest-plugin": "^4.0.2",
        "workbox-webpack-plugin": "^6.4.1"
    },
    "overrides": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^5.5.4",
        "fork-ts-checker-webpack-plugin": "^6.5.3",
